    <!-- ========== Small Pistol Bullets ========== -->
    <SimpleAmmo.AmmoDef ParentName="BaseAmmoIndustrial">
        <defName>Ammo_BulletPistolSmall</defName>
        <label>small pistol bullet</label>
        <labelShort>bullet</labelShort>
        <labelPlural>small pistol bullets</labelPlural>
        <description>A low-caliber pistol round, abstracted from rounds such as .22LR and 9x19mm parabellum. Used by smaller firearms such as the autopistol and machine pistol.</description>
        <graphicData>
            <texPath>Things/Item/Resource/Chocolate</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <statBases>
            <MarketValue>0.11</MarketValue>
            <Mass>0.01</Mass>
        </statBases>
    </SimpleAmmo.AmmoDef>

    <RecipeDef ParentName="BaseAmmoRecipeIndustrial">
        <defName>Make_Ammo_BulletPistolSmall</defName>
        <label>make small pistol bullets (x250)</label>
        <description>Make 250 small pistol bullets.</description>
        <jobString>Making small pistol bullets.</jobString>
        <targetCountAdjustment>250</targetCountAdjustment>
        <workAmount>2200</workAmount>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Steel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <Ammo_BulletPistolSmall>250</Ammo_BulletPistolSmall>
        </products>
    </RecipeDef>

